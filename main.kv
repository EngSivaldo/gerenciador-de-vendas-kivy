#:kivy 2.0.0
#:include kv/homepage.kv
#:include kv/ajustespage.kv

FloatLayout:
    canvas: 
        Color: 
            rgb: (0.0, 0.094, 0.141)  # Cor manualmente convertida do hexadecimal #001824 para RGB
        Rectangle:
            size: self.size 
            pos: self.pos
    Image:
        pos_hint: {'right':1, 'top': 0.9}
        size_hint: 1, 0.2
        source: 'icones/hash.png'
    ScreenManager:
        pos_hint: {'right': 1, 'top': 0.7}
        size_hint: 1, 0.7
        id: screen_manager
        HomePage:
            name: "homepage"
        AjustesPage:
            name: "ajustespage"
